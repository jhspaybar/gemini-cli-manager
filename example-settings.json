{
  "contextFileName": "GEMINI.md",
  "mcpServers": {
    "test-echo-server": {
      "command": "node",
      "args": ["./servers/echo-server.js"],
      "env": {
        "PORT": "3000",
        "LOG_LEVEL": "debug"
      },
      "cwd": "./servers",
      "timeout": 5000,
      "trust": false
    },
    "test-math-server": {
      "command": "python",
      "args": ["-m", "servers.math_server"],
      "env": {
        "PYTHON_PATH": "./servers"
      },
      "cwd": "./servers",
      "timeout": 5000,
      "trust": false
    }
  }
}