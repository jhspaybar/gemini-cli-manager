{
  "name": "github-tools",
  "version": "1.0.0",
  "description": "GitHub integration tools for Gemini CLI",
  "mcpServers": {
    "github-api": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-github"],
      "env": {
        "GITHUB_TOKEN": "$GITHUB_TOKEN"
      },
      "trust": false
    }
  },
  "contextFileName": "GITHUB.md"
}